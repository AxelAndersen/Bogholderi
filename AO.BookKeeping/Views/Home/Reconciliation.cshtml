@{
    ViewData["Title"] = "Bankafstemning";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<div>
    <form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="DoReconciliation">
        <div class="form-group">
            <div class="col-md-10">
                <p>Vælg excel fil til afstemning</p>
                <input type="file" name="reconciliationFile" />
            </div>
        </div>
        <div style="clear:both;"></div>
        <br />        
        <div class="form-group">
            <div class="col-md-10">
                <input type="submit" value="Start afstemning" />
            </div>
        </div>
    </form>
</div>